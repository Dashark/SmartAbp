@page
@using SmartAbp.Localization
@using SmartAbp.Web.Pages.Stations
@using Microsoft.Extensions.Localization
@model IndexModel
@inject IStringLocalizer<SmartAbpResource> L
@section scripts
{
    <abp-script src="/Pages/Stations/Index.js" />
}

<abp-card>
    <abp-card-header>
        <h2>@L["station列表"]</h2>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="stationTable"></abp-table>
    </abp-card-body>
</abp-card>